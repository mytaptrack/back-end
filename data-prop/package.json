{
    "name": "@mytaptrack/services-data-propogate",
    "version": "1.0.0",
    "scripts": {
        "start": "cdk deploy --watch --hotswap --all",
        "deploy": "cdk deploy --all",
        "codebuild": "npx samtsc --deploy-only",
        "build": "npx samtsc --build-only",
        "package": "npx samtsc --package",
        "prepackage-cdk": "rm -rf cdk.out",
        "package-cdk": "cdk deploy --no-execute --change-set-name pipeline-deployment --require-approval never",
        "start-cdk": "cdk deploy --watch --hotswap",
        "clean": "npx samtsc --deploy-only",
        "test": "jest --coverage",
        "lint": "tslint --config tslint.json --project tsconfig.json",
        "setDebug": "npm uninstall @mytaptrack/types @mytaptrack/lib @mytaptrack/cdk && npm i ../types ../lib ../cdk",
        "unsetDebug": "npm uninstall @mytaptrack/types @mytaptrack/lib @mytaptrack/cdk && npm i @mytaptrack/types @mytaptrack/lib @mytaptrack/cdk",
        "buildlibs": "cd ../types && tsc -d && cd ../lib && tsc -d",
        "stage-changeset": "cdk deploy --all --no-execute --change-set-name pipeline-deployment --require-approval never"
    },
    "dependencies": {
        "@aws-sdk/client-dynamodb": "^3.564.0",
        "@aws-sdk/client-eventbridge": "^3.564.0",
        "@aws-sdk/client-s3": "^3.564.0",
        "@aws-sdk/client-sfn": "^3.564.0",
        "@aws-sdk/lib-dynamodb": "^3.564.0",
        "@aws-sdk/util-dynamodb": "^3.564.0",
        "@lumigo/tracer": "^1.80.2",
        "@mytaptrack/cdk": "^0.0.38",
        "@mytaptrack/lib": "file:../lib",
        "@mytaptrack/types": "^2.0.31",
        "aws-cdk-lib": "^2.137.0",
        "cfn-response-promise": "^1.1.0",
        "constructs": "^10.0.0",
        "crypto-ts": "^1.0.2",
        "jsonschema": "^1.4.1",
        "lodash": "^4.17.21",
        "moment-timezone": "^0.5.34",
        "short-uuid": "^4.2.0",
        "source-map-support": "^0.5.21",
        "tslib": "^1.14.1",
        "uuid": "^8.3.2",
        "uuid4": "^1.1.4"
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.72",
        "@types/jest": "^29.5.12",
        "@types/lodash": "^4.17.0",
        "@types/node": "20.12.7",
        "aws-cdk": "2.138.0",
        "jest": "^29.7.0",
        "samtsc": "^1.1.18",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "typescript": "~5.4.5"
    }
}
