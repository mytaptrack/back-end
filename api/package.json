{
    "name": "mytaptrack-user-api",
    "version": "1.0.1",
    "scripts": {
        "start": "cdk deploy --watch --hotswap graphql",
        "deploy": "cdk deploy --all",
        "codebuild": "npx samtsc --deploy-only",
        "build": "cdk synth",
        "package": "npx samtsc --package",
        "prepackage-cdk": "rm -rf cdk.out",
        "package-cdk": "cdk deploy --no-execute --change-set-name pipeline-deployment --require-approval never",
        "start-cdk": "cdk deploy --watch --hotswap",
        "test": "jest --coverage",
        "presetDebug": "npm uninstall @mytaptrack/types",
        "setDebug": "npm i ../../types ../../cdk",
        "unsetDebug": "npm uninstall @mytaptrack/types npm i @mytaptrack/types --save",
        "buildlibs": "cd ../types && tsc -d && cd ../lib && tsc -d",
        "stage-changeset": "cdk deploy --all --no-execute --change-set-name pipeline-deployment --require-approval never",
        "local-api": "npx ts-node src/start-api.ts"
    },
    "dependencies": {
        "@aws-appsync/utils": "^1.5.0",
        "@aws-sdk/client-cognito-identity-provider": "^3.391.0",
        "@aws-sdk/client-dynamodb": "^3.391.0",
        "@aws-sdk/client-eventbridge": "^3.391.0",
        "@aws-sdk/client-kms": "^3.391.0",
        "@aws-sdk/client-lex-runtime-service": "^3.782.0",
        "@aws-sdk/client-pinpoint": "^3.502.0",
        "@aws-sdk/client-s3": "^3.391.0",
        "@aws-sdk/client-secrets-manager": "^3.529.1",
        "@aws-sdk/client-ses": "^3.391.0",
        "@aws-sdk/client-sesv2": "^3.782.0",
        "@aws-sdk/client-sfn": "^3.782.0",
        "@aws-sdk/client-sms": "^3.391.0",
        "@aws-sdk/client-sns": "^3.391.0",
        "@aws-sdk/client-sqs": "^3.391.0",
        "@aws-sdk/client-ssm": "^3.391.0",
        "@aws-sdk/client-timestream-query": "^3.391.0",
        "@aws-sdk/client-timestream-write": "^3.391.0",
        "@aws-sdk/lib-dynamodb": "^3.391.0",
        "@aws-sdk/s3-request-presigner": "^3.400.0",
        "@lumigo/tracer": "^1.85.0",
        "@mytaptrack/cdk": "file:../cdk",
        "@mytaptrack/get-ssl-certificate": "^2.3.4",
        "@mytaptrack/lib": "file:../lib",
        "@mytaptrack/stack-lib": "file:src/library",
        "@mytaptrack/types": "file:../types",
        "aws-sigv4-fetch": "^2.1.1",
        "body-parser": "^1.20.2",
        "crypto-ts": "^1.0.2",
        "express": "^5.1.0",
        "graphql": "^14.7.0",
        "graphql-http": "^1.22.4",
        "graphql-request": "^6.1.0",
        "jsonschema": "^1.4.0",
        "lodash": "^4.17.21",
        "moment-timezone": "^0.5.43",
        "qrcode": "^1.5.4",
        "short-uuid": "^4.2.0",
        "source-map-support": "^0.5.16",
        "stripe": "^14.20.0",
        "tslib": "^2.6.2",
        "twilio": "^5.0.4",
        "uuid": "^8.3.2",
        "written-number": "^0.11.1",
        "xlsx": "^0.18.5"
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.108",
        "@types/jest": "^27.0.2",
        "@types/moment-timezone": "^0.5.30",
        "@types/node": "^16.10.3",
        "aws-cdk-lib": "^2.204.0",
        "aws-sdk-mock": "^5.1.0",
        "constructs": "^10.4.2",
        "jest": "^29.5.0",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.2.6",
        "typescript": "^5.0.4",
        "url": "^0.11.0"
    }
}
